{
    "name": "win-local-inc/message-bus",
    "description": "A package for message bus.",
    "keywords": [
        "message-bus"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Piotr Kiszka",
            "email": "piotrek@lifeinmobile.com",
            "role": "Programmer"
        }
    ],
    "autoload": {
        "psr-4": {
            "WinLocal\\MessageBus\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "WinLocal\\MessageBus\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WinLocal\\MessageBus\\MessageBusServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "post-autoload-dump": [
            "@php vendor/bin/testbench package:discover --ansi"
        ]
    },
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "aws/aws-sdk-php-laravel": "^3.6",
        "laravel/framework": "^v8.0.0|^v9.0.0|^v10.0.0"
    },
    "require-dev": {
        "laravel/pint": "^1.6",
        "nunomaduro/collision": "^7.4",
        "orchestra/testbench": "^8.3"
    }
}
